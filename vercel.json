{
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "ignoreCommand": "exit 0",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_PUBLIC_BASE_URL": "https://bind8.com",
    "NEXT_PUBLIC_VERCEL_URL": "${VERCEL_URL}"
  }
}
